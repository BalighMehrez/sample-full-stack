{
  "name": "sample-full-stack",
  "version": "0.0.0",
  "scripts": {
    "client:ng": "ng",
    "client:start": "ng serve",
    "client:build": "ng build",
    "client:test": "ng test",
    "client:lint": "ng lint",
    "client:e2e": "ng e2e",
    "server:build": "tsc -p ./src/server/tsconfig.build.json",
    "preserver:build": "rimraf dist/server",
    "server:format": "prettier --write \"src/server/app/**/*.ts\"",
    "server:start": "ts-node -r tsconfig-paths/register src/server/app/main.ts",
    "server:start:dev": "nodemon --config ./src/server/nodemon.json",
    "server:start:debug": "nodemon --config ./src/server/nodemon-debug.json",
    "preserver:start:prod": "rimraf dist/server && npm run server:build",
    "server:start:prod": "node dist/server/main.js",
    "dev": "concurrently \" ng serve \" \" npm run server:start:debug \" ",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.0",
    "@angular/common": "~9.1.0",
    "@angular/compiler": "~9.1.0",
    "@angular/core": "~11.0.5",
    "@angular/forms": "~9.1.0",
    "@angular/platform-browser": "~9.1.0",
    "@angular/platform-browser-dynamic": "~9.1.0",
    "@angular/router": "~9.1.0",
    "@nestjs/common": "^7.0.7",
    "@nestjs/core": "^7.0.7",
    "@nestjs/graphql": "^7.1.4",
    "@nestjs/platform-express": "^7.0.7",
    "@nestjs/typeorm": "^7.0.0",
    "@types/graphql": "^14.5.0",
    "apollo-server": "^2.11.0",
    "apollo-server-express": "^2.11.0",
    "dataloader": "^2.0.0",
    "graphql": "^14.6.0",
    "graphql-subscriptions": "^1.1.0",
    "graphql-tools": "^4.0.7",
    "pg": "^8.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.4",
    "sqlite3": "^4.1.1",
    "tslib": "^1.10.0",
    "typeorm": "^0.2.24",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.0",
    "@angular/cli": "~9.1.0",
    "@angular/compiler-cli": "~9.1.0",
    "@angular/language-service": "~9.1.0",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "concurrently": "^5.1.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~3.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "minimist": "^1.2.5",
    "nodemon": "^2.0.2",
    "protractor": "~5.4.3",
    "ts-node": "~8.3.0",
    "tsconfig-paths": "^3.9.0",
    "tslint": "~6.1.0",
    "typescript": "~3.8.3"
  }
}
